<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="600.0" prefWidth="1000.0"
            style="-fx-background-color: #AED6F1;"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.cloud.storage.client.gui.LoginController">
    <children>
        <PasswordField fx:id="" layoutX="400.0" layoutY="340.0" promptText="password">
            <font>
                <Font size="16.0"/>
            </font>
        </PasswordField>
        <TextField fx:id="" layoutX="400.0" layoutY="289.0" promptText="username">
            <font>
                <Font size="16.0"/>
            </font>
        </TextField>
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="400.0" layoutY="154.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@logo.png"/>
            </image>
        </ImageView>
        <Button fx:id="" layoutX="398.0" layoutY="389.0" mnemonicParsing="false" onAction="#signIn"
                prefHeight="25.0" prefWidth="200.0" text="Sign In"/>
    </children>
</AnchorPane>
